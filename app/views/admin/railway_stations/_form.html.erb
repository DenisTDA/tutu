<%= form_for [:admin, @railway_station] do |form| %>
  <%= render 'common/errors',  resource: railway_station %>

  <div class="field">
    <%= form.label t('.title') %>
    <%= form.text_field :title, id: :railway_station_title %>
  </div>
   
  <div class="actions">
    <%= form.submit "#{ @railway_station.new_record? ? 'Создать' : 'Изменить' }"%>
  </div>
<% end %>
